# Memory Management Challenge - Environment Variables

# Application Settings
APP_NAME=MemoryManagementChallenge
APP_ENV=local
APP_DEBUG=true

# Memory Configuration
MEMORY_PROFILING_ENABLED=true
MEMORY_LEAK_THRESHOLD=10485760
MEMORY_ALERT_THRESHOLD=52428800

# Xdebug Settings
XDEBUG_MODE=develop,debug,profile
XDEBUG_START_WITH_REQUEST=trigger

# Database (if needed)
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=memory_challenge
DB_USERNAME=root
DB_PASSWORD=

# Monitoring
MONITORING_LOG_LEVEL=INFO
MONITORING_REPORT_INTERVAL=60