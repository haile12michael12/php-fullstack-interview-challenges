{
    "name": "php-fullstack/grpc-chat",
    "description": "gRPC Chat Application with PHP Backend",
    "type": "project",
    "require": {
        "php": ">=7.3",
        "spiral/roadrunner": "^2.0",
        "spiral/roadrunner-grpc": "^2.0",
        "google/protobuf": "^3.17",
        "grpc/grpc": "^1.38"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "proto-gen": "protoc --php_out=./src/Generated --grpc_out=./src/Generated --plugin=protoc-gen-grpc=./vendor/bin/grpc_php_plugin ./proto/chat.proto"
    }
}